    export PATH=/usr/local/cuda-11/bin:$PATH
    export LD_LIBRARY_PATH=/usr/local/cuda-11/lib64:$LD_LIBRARY_PATH
fi

export LD_LIBRARY_PATH=/usr/lib32/:$LD_LIBRARY_PATH
