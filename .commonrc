export DOTFILE_PATH=$HOME/.dotfiles/

export LANG="en_US.UTF-8"
export LC_COLLATE="en_US.UTF-8"
export LC_CTYPE="en_US.UTF-8"
export LC_MESSAGES="en_US.UTF-8"
export LC_MONETARY="en_US.UTF-8"
export LC_NUMERIC="en_US.UTF-8"
export LC_TIME="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"

source $DOTFILE_PATH/env
source $DOTFILE_PATH/path
source $DOTFILE_PATH/alias
source $DOTFILE_PATH/function
source $DOTFILE_PATH/misc
source $DOTFILE_PATH/tmp
test -f /etc/profile.d/rvm.sh && source /etc/profile.d/rvm.sh






